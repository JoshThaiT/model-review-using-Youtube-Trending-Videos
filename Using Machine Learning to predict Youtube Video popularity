   "source": [
    "#### Days Trending"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 107,
   "id": "93343015-fc7b-4840-88c6-9815c3f699c7",
   "metadata": {},
   "outputs": [],
   "source": [
    "from datetime import datetime\n",
    "\n",
    "def convert_date(date):\n",
    "    simple_date = datetime.fromisoformat(date.replace(\"Z\",\"+00:00\")).date()\n",
    "    return simple_date\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 108,
   "id": "e957305b-bdf5-4205-9689-44717afd1488",
   "metadata": {},
   "outputs": [],
   "source": [
    "df5['published_date']= df5['publishedAt'].apply(convert_date)"
   ]
  },
