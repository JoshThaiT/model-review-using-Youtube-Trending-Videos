   "id": "9e1b8703-ea68-4c21-9650-a20af8a99f7f",
   "metadata": {},
   "outputs": [],
   "source": [
    "df=pd.DataFrame()\n",
    "\n",
    "filenames = []\n",
    "\n",
    "#for  filename in os.listdir('C:/Users/J/OneDrive/WORK/Data Science/IOD/Mini Project 2.v3/data'):\n",
    "for  filename in os.listdir('C:/Users/TechFast Australia/OneDrive/WORK/Data Science/IOD/Mini Project 2.v3/data'):\n",
    "    filenames.append(filename)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "45fd871c-f304-43f0-846b-a640e19294c9",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/html": [
