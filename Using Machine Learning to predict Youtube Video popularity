       " ('la', 3606),\n",
       " ('short', 3310),\n",
       " ('–≤', 3014),\n",
       " ('que', 3012),\n",
       " ('vs', 2627),\n",
       " ('e', 2624),\n",
       " ('o', 2535),\n",
       " ('live', 2494),\n",
       " ('–∏', 2236),\n",
       " ('new', 2161),\n",
       " ('y', 2111),\n",
       " ('roblox', 2110),\n",
       " ('el', 2102),\n",
       " ('channel', 2056)]"
      ]
     },
     "execution_count": 106,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "word_freq = Counter(all_tokens)\n",
    "word_freq.most_common(20)"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "76c9bc24-49ad-4686-bc60-10d67c561659",
