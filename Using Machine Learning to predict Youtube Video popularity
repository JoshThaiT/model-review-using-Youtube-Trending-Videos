  {
   "cell_type": "code",
   "execution_count": 102,
   "id": "2e4384a7-92b2-4de7-a4e8-00cefc6c2bb2",
   "metadata": {},
   "outputs": [],
   "source": [
    "def tokenize(text):\n",
    "    tokens = text.split()\n",
    "    return tokens\n",
    "\n",
    "columns = [\"clean_title\", \"clean_description\", \"clean_tags\"]\n",
    "\n",
    "df5[\"all_tokens\"] = df5[columns].apply(lambda row: tokenize(\" \".join(row)), axis=1)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 103,
   "id": "28f3bd35-04e1-4275-b97f-21bc9a1291cc",
   "metadata": {},
   "outputs": [],
   "source": [
    "all_tokens = [token for tokens in df5[\"all_tokens\"] for token in tokens]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 106,
   "id": "bad00cd7-b670-40ac-a7f6-36f8794d9d5b",
   "metadata": {},
   "outputs": [
    {
     "data": {
