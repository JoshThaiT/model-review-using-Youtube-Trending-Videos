   "id": "a9c0d775-10d3-46e8-8af5-55de2fca79e9",
   "metadata": {},
   "source": [
    "### Feature Engineering"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "92caba82-5e0e-4fa8-8c82-d50a3ed8eea7",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "47126          P0D\n",
       "24471    PT2H29M9S\n",
       "24649     PT1H1M5S\n",
       "10702      PT3M37S\n",
       "4125     PT2H1M56S\n",
       "           ...    \n",
       "39350        PT11S\n",
       "28207        PT10S\n",
       "27722      PT1M12S\n",
       "25781        PT36S\n",
       "37890        PT47S\n",
       "Name: duration, Length: 16898, dtype: object"
      ]
     },
     "execution_count": 44,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "df1['duration']"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 45,
   "id": "cf454421-f32f-46d2-9fdc-5c43f075a422",
   "metadata": {},
   "outputs": [],
   "source": [
    "import re\n",
    "\n",
    "def convert_duration(duration):\n",
    "    regex=r'PT(?:(\\d+)H)?(?:(\\d+)M)?(?:(\\d+)S)?'\n",
    "    match = re.match(regex,duration)\n",
    "\n",
    "    if match:\n",
    "        hours = int(match.group(1) or 0)\n",
    "        minutes = int(match.group(2) or 0)\n",
    "        seconds = int(match.group(3) or 0)\n",
    "\n",
    "        return ((hours*3600) + (minutes*60) + seconds)\n",
    "        "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 46,
   "id": "852c3166-e4a3-46a7-aad8-183e4e4c2129",
   "metadata": {},
   "outputs": [],
   "source": [
    "df2=df1.copy()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 47,
   "id": "0830a43b-68bc-4146-86e6-2c5c9b20850b",
   "metadata": {},
   "outputs": [],
   "source": [
    "df2['durations_sec']=df2['duration'].apply(convert_duration)"
   ]
  },
