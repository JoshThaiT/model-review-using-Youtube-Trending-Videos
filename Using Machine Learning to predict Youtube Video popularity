   "outputs": [],
   "source": [
    "preprocessor_NB = ColumnTransformer(\n",
    "    transformers=[\n",
    "        ('cat_cat',lcatPipeline,cat_cat),\n",
    "        ('num',num_pre_min_max,num_cat),\n",
    "        ('text',txtPipeline,'clean_tags'),\n",
    "        ('text1',txtPipeline,'clean_title'),\n",
    "        ('text2',txtPipeline,'clean_description')\n",
    "    ],\n",
    "    remainder='drop'\n",
    ")"
   ]
  },
  {
